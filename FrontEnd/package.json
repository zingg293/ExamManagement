{
    "name": "human-resource-management-system",
    "private": true,
    "type": "module",
    "version": "0.0.1",
    "description": "A React typescript vite js project, with antd,eslint,prettier,so on",
    "license": "MIT",
    "author": "khanhdoan693@gmail.com",
    "scripts": {
        "dev": "vite --open",
        "build": "tsc && vite build",
        "preview": "yarn build && vite preview --host",
        "lint": "eslint --ext ts,tsx src/",
        "lint:fix": "eslint --fix --ext ts,tsx src/",
        "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format": "npm run prettier:fix && npm run lint:fix",
        "Remove-Node-Modules": "npx rimraf --glob **/node_modules",
        "Yarn cache clean": "yarn cache clean"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.0.8",
        "@dnd-kit/modifiers": "^6.0.1",
        "@dnd-kit/sortable": "^7.0.2",
        "@reduxjs/toolkit": "^1.9.5",
        "antd": "^5.8.3",
        "antd-img-crop": "^4.12.2",
        "axios": "^1.4.0",
        "braft-editor": "^2.3.9",
        "dayjs": "1.11.9",
        "rc-util": "^5.35.0",
        "react": "18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "18.2.0",
        "react-draggable": "^4.4.5",
        "react-error-boundary": "^4.0.4",
        "react-lazy-load-image-component": "^1.6.0",
        "react-redux": "8.1.1",
        "react-router-dom": "6.14.0",
        "react-scripts": "5.0.1",
        "redux-logger": "^3.0.6",
        "redux-saga": "1.2.3",
        "web-vitals": "3.4.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/node": "^20.2.3",
        "@types/react": "18.2.6",
        "@types/react-beautiful-dnd": "^13.1.4",
        "@types/react-dom": "18.2.4",
        "@types/react-lazy-load-image-component": "^1.5.3",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-logger": "^3.0.9",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "@vitejs/plugin-react": "^4.0.0",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.5.0",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4",
        "vite": "4.3.9",
        "vite-tsconfig-paths": "^4.2.0"
    }
}