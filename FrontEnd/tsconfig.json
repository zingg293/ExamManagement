{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "@admin/*": [
        "src/package/admin/*"
      ],
      "@client/*": [
        "src/package/client/*"
      ],
      "@units/*": [
        "src/units/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@API/*": [
        "src/API/*"
      ]
    }
  },
  "include": [
    "vite.config.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/package/admin/features/dashBoard/index.js"  ],
  "exclude": [
    "node_modules"
  ]
}
